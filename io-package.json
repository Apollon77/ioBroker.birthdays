{
    "common": {
        "name": "birthdays",
        "version": "0.0.1",
        "news": {
            "0.0.1": {
                "en": "initial release",
                "de": "Erstver√∂ffentlichung"
            }
        },
        "title": "birthdays",
        "titleLang": {
            "en": "birthdays",
            "de": "Geburtstage"
        },
        "desc": {
            "en": "Use an ical file to import your contacts birthdays",
            "de": "Verwenden Sie eine ical-Datei, um die Geburtstage Ihrer Kontakte zu importieren"
        },
        "authors": [
            "Matthias Kleine <info@haus-automatisierung.com>"
        ],
        "keywords": [
            "calendar",
            "birthday"
        ],
        "license": "MIT",
        "platform": "Javascript/Node.js",
        "main": "main.js",
        "icon": "birthdays.png",
        "enabled": true,
        "extIcon": "https://raw.githubusercontent.com/klein0r/ioBroker.birthdays/master/admin/birthdays.png",
        "readme": "https://github.com/klein0r/ioBroker.birthdays/blob/master/README.md",
        "loglevel": "info",
        "mode": "schedule",
        "allowInit": true,
        "schedule": "0 0 * * *",
        "type": "date-and-time",
        "compact": true,
        "connectionType": "cloud",
        "dataSource": "poll",
        "materialize": true,
        "dependencies": [
            {
                "js-controller": ">=2.0.0"
            }
        ]
    },
    "native": {
        "icalUrl": ""
    },
    "objects": [],
    "instanceObjects": [
        {
          "_id":  "next",
          "type": "channel",
          "common": {
            "name": "Next birthday(s)"
          },
          "native": {}
        },
        {
          "_id":  "summary",
          "type": "channel",
          "common": {
            "name": "Birthday summary"
          },
          "native": {}
        },
        {
            "_id": "summary.json",
            "type": "state",
            "common": {
                "name": "JSON string for tables",
                "type": "string",
                "role": "value",
                "read": true,
                "write": false
            },
            "native": {}
        }
    ]
}
