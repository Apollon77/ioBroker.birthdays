{
    "common": {
        "name": "birthdays",
        "version": "0.1.1",
        "news": {
            "0.1.1": {
                "en": "Added custom date format",
                "de": "Eigenes Datumsformat hinzugefügt"
            },
            "0.1.0": {
                "en": "Admin 5 Support",
                "de": "Admin 5 Support"
            },
            "0.0.3": {
                "en": "Added next and nextAfter birthdays",
                "de": "Nächste und übernächste Geburtstage errechnet"
            },
            "0.0.2": {
                "en": "Added more objects and states",
                "de": "Mehr Objekte und States zur Verfügung gestellt"
            },
            "0.0.1": {
                "en": "initial release",
                "de": "Erstveröffentlichung"
            }
        },
        "title": "Birthdays",
        "titleLang": {
            "en": "Birthdays",
            "de": "Geburtstage"
        },
        "desc": {
            "en": "Use an ical file to import your contacts birthdays",
            "de": "Verwenden Sie eine ical-Datei, um die Geburtstage Ihrer Kontakte zu importieren"
        },
        "authors": [
            "Matthias Kleine <info@haus-automatisierung.com>"
        ],
        "keywords": [
            "calendar",
            "birthday"
        ],
        "license": "MIT",
        "platform": "Javascript/Node.js",
        "main": "main.js",
        "icon": "birthdays.png",
        "extIcon": "https://raw.githubusercontent.com/klein0r/ioBroker.birthdays/master/admin/birthdays.png",
        "enabled": true,
        "readme": "https://github.com/klein0r/ioBroker.birthdays/blob/master/README.md",
        "loglevel": "info",
        "mode": "schedule",
        "allowInit": true,
        "schedule": "0 0 * * *",
        "type": "date-and-time",
        "compact": true,
        "connectionType": "cloud",
        "dataSource": "poll",
        "materialize": true,
        "adminUI": {
            "config": "json"
        },
        "globalDependencies": [
            {
                "admin": ">=5.1.19"
            }
        ],
        "dependencies": [
            {
                "js-controller": ">=3.3.0"
            }
        ]
    },
    "native": {
        "icalUrl": "",
        "nextTextTemplate": "%n turns %a",
        "nextSeparator": ", "
    },
